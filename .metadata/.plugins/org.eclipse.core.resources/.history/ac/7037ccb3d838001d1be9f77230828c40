package ejemplos;

import java.util.Scanner;

public class prueba {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.print("Escribe tu nombre: ");
		String nombre = s.nextLine();
		for (int i = 0; i < 5; i++) {
			System.out.println("Tu nombre es "+ nombre);
		}
	}

}
